<div class = "container">
<duv class= "span8">
<h1>Unreal Blog</h1>

<table class="table ">
  <thead>
    <tr >
      <th></th>
      <th></th>

      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr >
        <td><strong><%= link_to post.title, post %></strong></td>
        <td><%= post.text %></td>

        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
</div>
</div>